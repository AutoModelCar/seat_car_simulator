          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>100</mu>
                <mu2>50</mu2>
              </ode>
            </friction>
          </surface>
        </collision>
        <visual name="visual">
          <cast_shadows>false</cast_shadows>
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <material>
            <ambient>0.09 0.09 0.09 1</ambient>
            <diffuse>0.09 0.09 0.09 1</diffuse>
            <specular>0.0 0.0 0.0 1</specular>
            <emissive>0 0 0 0</emissive>
            <!--
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Black</name>
            </script>
            -->
          </material>
        </visual>
      </link>
    </model>

